{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "berkayk/onesignal-laravel": "^0.9.9",
        "fideloper/proxy": "~3.3",
        "fx3costa/laravelchartjs": "^2.2",
        "hipsterjazzbo/landlord": "^2.0",
        "intervention/image": "^2.3",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "^2.1",
        "milon/barcode": "^5.2",
        "predis/predis": "^1.1",
        "quickbooks/v3-php-sdk": "^5.0",
        "regulus/activity-log": "0.6.*",
        "reportico/laravel-reportico": "~5.2",
        "riverskies/laravel-mobile-detect": "^1.3",
        "simplesoftwareio/simple-qrcode": "~1",
        "spatie/laravel-activitylog": "^2.2",
        "spatie/laravel-backup": "^5.11",
        "vsmoraes/laravel-pdf": "^2.0",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "OrionMedical\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
